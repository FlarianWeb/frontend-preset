{
	"name": "@flarian/frontend-preset",
	"version": "1.1.1",
	"license": "MIT",
	"author": "FlarianWeb",
	"description": "Frontend preset with ESLint 9 flat config, Prettier and Stylelint",
	"keywords": [
		"eslint",
		"prettier",
		"stylelint",
		"vue",
		"frontend",
		"preset",
		"linting"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"type": "module",
	"exports": {
		".": {
		"types": "./dist/index.d.ts",
		"require": "./dist/index.cjs",
		"import": "./dist/index.mjs"
		},
		"./eslint": {
		"types": "./dist/eslint/index.d.ts",
		"require": "./dist/eslint/index.cjs",
		"import": "./dist/eslint/index.mjs"
		},
		"./eslint/*": {
		"types": "./dist/eslint/config/*.d.ts",
		"require": "./dist/eslint/config/*.cjs",
		"import": "./dist/eslint/config/*.mjs"
		},
		"./prettier": {
		"types": "./dist/prettier/index.d.ts",
		"require": "./dist/prettier/index.cjs",
		"import": "./dist/prettier/index.mjs"
		},
		"./stylelint": {
		"types": "./dist/stylelint/index.d.ts",
		"require": "./dist/stylelint/index.cjs",
		"import": "./dist/stylelint/index.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild"
	},
	"devDependencies": {
		"@eslint/js": "9.35.0",
		"@semantic-release/changelog": "6.0.3",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "11.0.5",
		"@semantic-release/npm": "12.0.2",
		"@stylistic/eslint-plugin": "5.3.1",
		"@types/eslint": "9.6.1",
		"conventional-changelog-conventionalcommits": "9.1.0",
		"eslint": "9.35.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-jsonc": "2.20.1",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-vue": "10.4.0",
		"prettier": "3.6.2",
		"semantic-release": "24.2.8",
		"stylelint": "16.24.0",
		"stylelint-config-standard": "39.0.0",
		"typescript": "5.9.2",
		"typescript-eslint": "8.43.0",
		"unbuild": "3.6.1"
	},
	"engines": {
		"node": ">=22.5.0"
	},
	"packageManager": "pnpm@10.6.1",
	"peerDependencies": {
		"@eslint/js": ">=9.30.0",
		"@stylistic/eslint-plugin": ">=5.0.0",
		"eslint": ">=9.30.0",
		"eslint-config-prettier": ">=10.1.0",
		"eslint-plugin-jsonc": ">=2.20.0",
		"eslint-plugin-prettier": ">=5.5.0",
		"eslint-plugin-simple-import-sort": ">=12.1.0",
		"eslint-plugin-vue": ">=10.0.0",
		"prettier": ">=3.0.0",
		"stylelint": ">=16.20.0",
		"stylelint-config-standard": ">=39.0.0",
		"typescript-eslint": ">=8.15.0"
	},
	"peerDependenciesMeta": {
		"eslint-plugin-vue": {
			"optional": true
		},
		"eslint-plugin-jsonc": {
			"optional": true
		},
		"eslint-plugin-prettier": {
			"optional": true
		},
		"eslint-plugin-simple-import-sort": {
			"optional": true
		},
		"typescript-eslint": {
			"optional": true
		},
		"stylelint": {
			"optional": true
		},
		"stylelint-config-standard": {
			"optional": true
		}
	}
}
