{
	"name": "@flarian/frontend-preset",
	"version": "1.2.1",
	"license": "MIT",
	"description": "Frontend preset with ESLint 9 flat config and Stylelint",
	"author": "FlarianWeb",
	"keywords": [
		"eslint",
		"stylelint",
		"vue",
		"frontend",
		"preset",
		"linting"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./eslint": {
			"types": "./dist/eslint/index.d.ts",
			"require": "./dist/eslint/index.cjs",
			"import": "./dist/eslint/index.mjs"
		},
		"./eslint/*": {
			"types": "./dist/eslint/config/*.d.ts",
			"require": "./dist/eslint/config/*.cjs",
			"import": "./dist/eslint/config/*.mjs"
		},
		"./prettier": {
			"types": "./dist/prettier/index.d.ts",
			"require": "./dist/prettier/index.cjs",
			"import": "./dist/prettier/index.mjs"
		},
		"./prettier/*": {
			"types": "./dist/prettier/config/*.d.ts",
			"require": "./dist/prettier/config/*.cjs",
			"import": "./dist/prettier/config/*.mjs"
		},
		"./stylelint": {
			"types": "./dist/stylelint/index.d.ts",
			"require": "./dist/stylelint/index.cjs",
			"import": "./dist/stylelint/index.mjs"
		},
		"./stylelint/*": {
			"types": "./dist/stylelint/config/*.d.ts",
			"require": "./dist/stylelint/config/*.cjs",
			"import": "./dist/stylelint/config/*.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"lint": "eslint",
		"typecheck": "tsc --noEmit"
	},
	"engines": {
		"node": ">=22.5.0"
	},
	"packageManager": "pnpm@10.6.1",
	"devDependencies": {
		"@eslint/js": "9.35.0",
		"@prettier/plugin-pug": "3.4.2",
		"@semantic-release/changelog": "6.0.3",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "11.0.6",
		"@semantic-release/npm": "12.0.2",
		"@stylistic/eslint-plugin": "5.3.1",
		"@types/eslint": "9.6.1",
		"conventional-changelog-conventionalcommits": "9.1.0",
		"eslint": "9.35.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-jsonc": "2.20.1",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-vue": "10.4.0",
		"globals": "16.4.0",
		"prettier": "3.6.2",
		"semantic-release": "24.2.8",
		"stylelint": "16.24.0",
		"stylelint-config-recommended-vue": "1.6.1",
		"stylelint-config-standard": "39.0.0",
		"stylelint-config-standard-less": "3.0.1",
		"stylelint-config-standard-scss": "16.0.0",
		"stylelint-less": "3.0.1",
		"stylelint-order": "7.0.0",
		"stylelint-scss": "6.12.1",
		"typescript": "5.9.2",
		"typescript-eslint": "8.44.0",
		"unbuild": "3.6.1",
		"vue-eslint-parser": "10.2.0"
	},
	"peerDependencies": {
		"@eslint/js": ">=9.30.0",
		"@prettier/plugin-pug": ">=3.3.0",
		"@stylistic/eslint-plugin": ">=5.0.0",
		"eslint": ">=9.30.0",
		"eslint-config-prettier": ">=10.1.0",
		"eslint-plugin-jsonc": ">=2.20.0",
		"eslint-plugin-prettier": ">=5.4.0",
		"eslint-plugin-react": ">=7.37.0",
		"eslint-plugin-react-hooks": ">=5.2.0",
		"eslint-plugin-simple-import-sort": ">=12.1.0",
		"eslint-plugin-vue": ">=10.0.0",
		"globals": ">=14.0.0",
		"prettier": ">=3.5.0",
		"stylelint": ">=16.20.0",
		"stylelint-config-recommended-vue": ">=1.6.0",
		"stylelint-config-standard": ">=39.0.0",
		"stylelint-config-standard-less": ">=3.0.1",
		"stylelint-config-standard-scss": ">=15.0.0",
		"stylelint-less": ">=3.0.1",
		"stylelint-order": ">=7.0.0",
		"stylelint-scss": ">=6.12.0",
		"typescript-eslint": ">=8.15.0",
		"vue-eslint-parser": ">=10.0.0"
	},
	"peerDependenciesMeta": {
		"eslint": {
			"optional": true
		},
		"prettier": {
			"optional": true
		},
		"stylelint": {
			"optional": true
		}
	}
}
