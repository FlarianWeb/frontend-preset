{
	"name": "@flarian/frontend-preset",
	"version": "1.1.1",
	"license": "MIT",
	"author": "FlarianWeb",
	"description": "Frontend preset with ESLint 9 flat config, Prettier and Stylelint",
	"keywords": [
		"eslint",
		"prettier",
		"stylelint",
		"vue",
		"frontend",
		"preset",
		"linting"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"type": "module",
	"files": [ "dist" ],
	"exports": {
		"./eslint": {
			"types": "./dist/eslint/index.d.ts",
			"default": "./dist/eslint/index.js"
		},
		"./prettier": {
			"types": "./dist/prettier/index.d.ts",
			"default": "./dist/prettier/index.js"
		},
		"./stylelint": {
			"types": "./dist/stylelint/index.d.ts",
			"default": "./dist/stylelint/index.js"
		}
	},
	"scripts": {
		"build": "tsc"
	},
	"devDependencies": {
		"@eslint/js": "9.34.0",
		"@semantic-release/changelog": "6.0.3",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "11.0.5",
		"@semantic-release/npm": "12.0.2",
		"@stylistic/eslint-plugin": "5.2.3",
		"@types/eslint": "9.6.1",
		"conventional-changelog-conventionalcommits": "9.1.0",
		"eslint": "9.34.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-jsonc": "2.20.1",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-vue": "10.4.0",
		"prettier": "3.6.2",
		"semantic-release": "24.2.7",
		"stylelint": "16.23.1",
		"stylelint-config-standard": "39.0.0",
		"typescript": "5.9.2",
		"typescript-eslint": "8.41.0"
	},
	"engines": {
		"node": ">=22.5.0"
	},
	"packageManager": "pnpm@10.6.1",
	"peerDependencies": {
		"@eslint/js": "9.34.0",
		"@stylistic/eslint-plugin": "5.2.3",
		"eslint": "9.34.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-jsonc": "2.20.1",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-vue": "10.4.0",
		"prettier": "^3.0.0",
		"stylelint": "16.23.1",
		"stylelint-config-standard": "39.0.0",
		"typescript-eslint": "8.41.0"
	}
}
